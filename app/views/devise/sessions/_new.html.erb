<!--Body-->
<div id="login-page" class="modal-body">
  <div class="row">
    <div class="col-md-12">
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
              <h4 class="text-center">or:</h4>
        <div class="input-field">
          <i class="material-icons prefix">email</i>
          <%= f.label "Your email", for: "icon_email" %>
          <%= f.email_field :email, autofocus: false, id: "icon_email", class: "validate" %>
        </div>
        <div class="input-field">
          <i class="material-icons prefix">fingerprint</i>
          <%= f.label "Your password" %>
          <%= f.password_field :password, autocomplete: false, class: "validate" %>
        </div>
        <div class="text-center">
          <button onclick="hideLoginPage()" id="sign-up" type="button" class="btn btn-primary btn-lg btn-ptc waves-effect waves-light">Sign up</button>
          <%= f.button  "Login", class: "btn btn-primary btn-lg btn-ptc waves-effect waves-light" %>
          <div class="subscription-checkbox">
            <form action="#">
              <input type="checkbox" id="test7" />
              <label for="test7">
                <p>Remember Me</p>
              </label>
            </form>
          </div>
        <% end %>
        <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
</div>
<!--Form-->